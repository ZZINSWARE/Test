    ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 0, 0)),       -- Black
    ColorSequenceKeypoint.new(0.5, Color3.fromRGB(128, 128, 128)), -- Grey
    ColorSequenceKeypoint.new(1, Color3.fromRGB(255, 255, 255))    -- White
})
textGradient.Rotation = 45

